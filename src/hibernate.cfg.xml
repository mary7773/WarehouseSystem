<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.microsoft.sqlserver.jdbc.SQLServerDriver</property>
        <property name="hibernate.connection.password">123</property>
        <property name="hibernate.connection.url">jdbc:sqlserver://localhost:1433;databaseName=warehouseDB</property>
        <property name="hibernate.connection.username">marieta</property>
        <property name="hibernate.dialect">org.hibernate.dialect.SQLServerDialect</property>
        <property name="hibernate.search.autoregister_listeners">false</property>
        <mapping resource="entity/Brands.hbm.xml" />
        <mapping resource="entity/Users.hbm.xml" />
        <mapping resource="entity/Invoice.hbm.xml" />
        <mapping resource="entity/Cities.hbm.xml" />
        <mapping resource="entity/Sysdiagrams.hbm.xml" />
        <mapping resource="entity/Applicationlog.hbm.xml" />
        <mapping resource="entity/Products.hbm.xml" />
        <mapping resource="entity/UsersGroups.hbm.xml" />
        <mapping resource="entity/Countries.hbm.xml" />
        <mapping resource="entity/TypePayment.hbm.xml" />
        <mapping resource="entity/Categories.hbm.xml" />
        <mapping resource="entity/Orders.hbm.xml" />
        <mapping resource="entity/Payment.hbm.xml" />
    </session-factory>
</hibernate-configuration>
